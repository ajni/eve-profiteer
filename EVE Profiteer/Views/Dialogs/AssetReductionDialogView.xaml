<dx:DXDialog x:Class="eZet.EveProfiteer.Views.Dialogs.AssetReductionDialogView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
             xmlns:dialogs="clr-namespace:eZet.EveProfiteer.ViewModels.Dialogs"
             mc:Ignorable="d"
             d:DataContext="{d:DesignInstance Type=dialogs:AssetReductionDialogViewModel, IsDesignTimeCreatable=False}"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>

        <DockPanel>
            <StackPanel DockPanel.Dock="Top" Margin="10">
                <TextBlock Text="Quantity" />
                <TextBox Name="Quantity" Text="{Binding Quantity}" />
                <TextBlock Text="Description" Margin="0 20 0 0" />
                <TextBox Name="Description" Text="{Binding Description}" />
            </StackPanel>
    
        </DockPanel>
    </Grid>

</dx:DXDialog>